{
  "build": {
    "core": "nRF5",
    "cpu": "cortex-m4",
    "extra_flags": "-DNRF52 -DNRF52840 -DNRF52840_XXAA -DMYBOARDNRF52840 -I$PROJECT_DIR/lib/MyBoardNRF52840 -L$PROJECT_DIR/lib/MyBoardNRF52840",
    "f_cpu": "64000000L",
    "ldscript": "nrf52840_xxaa.ld",
    "mcu": "nrf52840",
    "variant": "MyBoardNRF52840"
  },
  "connectivity": [
    "bluetooth"
  ],
  "debug": {
    "jlink_device": "nRF52840_xxAA",
    "svd_path": "nrf52840.svd",
    "default_tools": [
      "openocd",
      "jlink",
      "blackmagic"
    ]
  },
  "frameworks": [
    "arduino"
  ],
  "name": "MyBoardNRF52840 (MySensors)",
  "upload": {
    "maximum_ram_size": 262144,
    "maximum_size": 1048576,
    "protocol": "jlink",
    "protocols": [
      "jlink",
      "nrfjprog",
      "stlink",
      "blackmagic"
    ]
  },
  "url": "https://www.mysensors.org",
  "vendor": "MySensors"
}
